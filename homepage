    <script>
        feather.replace();
        
        // Initialize Vanta.js background
        VANTA.NET({
            el: "#vanta-bg",
            mouseControls: true,
            touchControls: true,
            gyroControls: false,
            minHeight: 200.00,
            minWidth: 200.00,
            scale: 1.00,
            scaleMobile: 1.00,
            color: 0xd4af37,
            backgroundColor: 0x111827,
            points: 12.00,
            maxDistance: 24.00,
            spacing: 18.00
        });
        
        // Parallax effect
        document.addEventListener('mousemove', (e) => {
            const layers = document.querySelectorAll('.parallax-layer');
            const x = (window.innerWidth - e.pageX * 0.1) / 100;
            const y = (window.innerHeight - e.pageY * 0.1) / 100;
            
            layers.forEach(layer => {
                const speed = parseFloat(layer.getAttribute('data-speed'));
                layer.style.transform = `translate(${x * speed}px, ${y * speed}px)`;
            });
        });
        
        // Glow animation for heading
        anime({
            targets: '.glow',
            opacity: [0.9, 1],
            duration: 2000,
            direction: 'alternate',
            loop: true,
            easing: 'easeInOutSine'
        });
    </script>
